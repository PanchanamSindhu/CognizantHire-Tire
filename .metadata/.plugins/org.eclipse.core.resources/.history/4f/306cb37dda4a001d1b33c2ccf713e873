import static org.junit.jupiter.api.Assertions.*;

import java.util.ArrayList;
import java.util.List;

import org.junit.jupiter.api.Test;

import com.cog.test.Sample1;

class JunitTestCases {

	@Test
	public void addNumbers() {
		System.out.println("test case for Role of the employee");
		List<Employee> empList = new ArrayList<Employee>();
		Employee e1 = new Employee("John", 23, 14000, "SE");
		Employee e2 = new Employee("Bob", 24, 25000, "analyst");
		Employee e3 = new Employee("Tom", 25, 26000, "SE");
		empList.add(e1);
		empList.add(e2);
		empList.add(e3);
		assertEquals(2, TestClass.empRole(empList).size());

	}

}
